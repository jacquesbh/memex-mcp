{
    "output": "memex",
    "main": "castor.php",
    "compression": "GZ",
    "directories": [
        "src",
        "config"
    ],
    "files": [
        "castor.php",
        ".castor.stub.php",
        ".castor.logo.php"
    ],
    "finder": [
        {
            "name": "*.php",
            "in": ["src"]
        }
    ],
    "stub": ".castor.stub.php",
    "banner": [
        "MEMEX v1.0.0",
        "Memory + Index | Knowledge Base for AI",
        "Built with Castor + Symfony MCP SDK"
    ]
}
