{
    "name": "monsieurbiz/memex-mcp",
    "description": "MEMEX - MCP server for managing guides and contexts knowledge base. Inspired by Vannevar Bush's vision of augmented memory.",
    "type": "project",
    "require": {
        "php": "^8.3",
        "symfony/console": "^7.2",
        "symfony/framework-bundle": "^7.2",
        "symfony/mcp-sdk": "dev-main",
        "claude-php/claude-3-api": "^0.1.23",
        "league/commonmark": "^2.7",
        "symfony/dotenv": "^7.3",
        "symfony/yaml": "^7.3",
        "symfony/dependency-injection": "^7.3",
        "symfony/http-kernel": "^7.3",
        "symfony/config": "^7.3",
        "symfony/finder": "^7.2"
    },
    "authors": [
        {
            "name": "MonsieurBiz"
        }
    ],
    "autoload": {
        "psr-4": {
            "Memex\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Memex\\Tests\\": "tests/"
        }
    },
    "config": {
        "sort-packages": true,
        "preferred-install": "dist"
    }
}
